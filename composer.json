{
    "name": "gamringer/php-rest",
    "description": "PHP REST Framework",
    "keywords": ["json", "json pointer", "RFC6901", "6901", "rest"],
    "homepage": "https://github.com/gamringer/PHPREST",
    "license": "MIT",
    "authors": [
        {
            "name": "Guillaume Amringer",
            "email": "g.amringer@gmail.com"
        }
    ],
    "require": {
        "php": "~7.0",
        "gamringer/php-json-pointer": "@stable",
        "guzzlehttp/psr7": "@stable",
        "league/container": "^2.0",
        "gamringer/pipe": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "http-interop/http-factory-guzzle": "^1.0",
        "phpstan/phpstan": "^0.10.5",
        "phpmd/phpmd": "^2.6",
        "phan/phan": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "gamringer\\PHPREST\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "gamringer\\PHPREST\\Test\\": "tests/",
            "gamringer\\PHPREST\\Example\\": "examples/"
        }
    }

}
